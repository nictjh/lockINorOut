generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Article {
  id        Int      @id @default(autoincrement())
  title     String
  url       String   @unique
  snippet   String?
  category  String?
  topic     String
  source    String?
  createdAt DateTime @default(now())
}
